{
    "name": "twokings/bolt-searchable-content-extension",
    "description": "Searchable Content Extension allows you to search in the content of configured repeaters and blocks",
    "type": "bolt-extension",
    "keywords": [
        "search",
        "content",
        "searchable",
        "blocks",
        "repeaters"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao@twokings.nl"
        },
        {
            "name": "Néstor de Dios Fernández",
            "email": "nestor@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\TwoKings\\SearchableContent\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\TwoKings\\SearchableContent\\SearchableContentExtension",
        "bolt-icon": "bolt-icon.png"
    }
}
